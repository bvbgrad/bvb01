<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appenders>
        <Console name="console">
            <PatternLayout pattern="%d{yyyy-mm-dd HH:mm:ss.SSS} %-5level %logger{32} %msg%n" />
        </Console>
    </appenders>
    <loggers>
         <root level="info">
            <appender-ref ref="console" />
        </root>
    </loggers>
</configuration>
